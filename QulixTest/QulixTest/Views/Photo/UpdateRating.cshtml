@model Endpoins.Models.PhotoModel

<h2>Change Photo @Model.Name.</h2>
<h3>Input Rating from 0 to 10</h3>
<body>
<form asp-action="UpdateRating" asp-controller="Photo" method="post">
    <div>
        
        <div class="form-group">
            <label for="Rating">Rating</label>
            <input name="Rating" class="form-control" />
        </div>
        <input type="submit" class="btn btn-default" value="submit" />
    </div>
</form>
</body>
<div>
 <a href="https://localhost:44346/api/photo">
<input type="button" value="Back">
</a>
</div>