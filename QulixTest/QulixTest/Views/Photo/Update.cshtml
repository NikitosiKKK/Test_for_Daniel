@model Endpoins.Models.PhotoModel

<h2>Change Photo @Model.Name.</h2>
<h3>Input all columns</h3>
<body>
<form asp-action="Update" asp-controller="Photo" method="post">
    <div>
        
        <div class="form-group">
            <label for="Name">Name</label>
            <input type="text" name="Name" class="form-control" />
        </div>
        <div class="form-group">
            <label for="Url">Url</label>
            <input type="text" name="Url" class="form-control" />
        </div>
        <div class="form-group">
            <label for="Size">Size</label>
            <input type="text" name="Size" class="form-control" />
        </div>
       
        <div class="form-group">
            <label for="Price">Price</label>
            <input name="Price" class="form-control" />
        </div>
        <div class="form-group">
            <label for="Purchases">Purchases</label>
            <input name="Purchases" class="form-control" />
        </div>
        <input type="submit" class="btn btn-default" value="submit" />

    </div>
</form>
</body>
<div>
<a href="https://localhost:44346/api/photo">
<input type="button" value="Back">
</a>
</div>